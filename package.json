{
  "name": "voiceci",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "db:generate": "pnpm --filter @voiceci/db generate",
    "db:migrate": "pnpm --filter @voiceci/db migrate"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "turbo": "^2.3.0",
    "typescript": "^5.7.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.15.0",
  "pnpm": {
    "overrides": {
      "ioredis": "5.9.3"
    }
  }
}
